{
  "destination": {
    "host": "127.0.0.1",
    "port": 514,
    "protocol": "tcp"
  },
  "tls": {
    "enabled": false,
    "ca_file": null,
    "cert_file": null,
    "key_file": null,
    "verify_mode": "required"
  },
  "files": {
    "pattern": "*.log",
    "read_from_beginning": true,
    "rescan_interval_sec": 5.0
  },
  "syslog": {
    "facility": "mocker",
    "app_name": "LP-Mocker",
    "host_name": "Demo",
    "severity": "info"
  },
  "webhook": {
    "url": "localhost/api/webhook/incoming",
    "method": "POST",
    "headers": {},
    "mps": 1.0
  }
}